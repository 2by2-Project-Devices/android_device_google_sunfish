type logger_app, domain;
dontaudit logger_app activity_service:service_manager find;
dontaudit logger_app binder_device:chr_file ioctl;
dontaudit logger_app system_file:file getattr;
dontaudit logger_app tmpfs:file { execute map read write };
dontaudit logger_app usb_service:service_manager find;
dontaudit system_server logger_app:binder call;
dontaudit zygote logger_app:process { dyntransition setpgid };
